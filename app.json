{
	"expo": {
		"name": "GeoGuesser",
		"slug": "geo-guesser",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#26414A"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.geoguesser.app",
			"buildNumber": "1.0.0"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#26414A"
			},
			"package": "com.geoguesser.app",
			"versionCode": 1
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["@react-native-firebase/app"]
	}
}
